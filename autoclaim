function bal(){
$dash=auto();
	$bul=explode('<span>',explode('Available Faucet Token</p>',$dash)[1])[0];
	$bl=explode('<span>FCT',explode('<p class="amount">',$dash)[1])[0];
fast("\033[1;32m>_ Balance: \033[1;37m$bl FCT TOKEN\n");
}

$db=db();
$user=explode('</p>',explode('<p class="username">',$db)[1])[0];
fast("\033[1;32m>_ Username: \033[1;37m$user \n");
bal();
garis();
while(true){
$res=faucet();
	$warn=explode('</p>',explode('<p class="warning">',$res)[1])[0];
	if($warn){
		if(preg_match('/insufficient FCT token balance/',$res)){
			$warn = str_replace('! ',"!\n",$warn);
			echo">_{$putih} $warn\n";
			garis();exit;
		}
		if(preg_match('/Please wait until time is up!/',$res)){
			$warn = str_replace('! ',"!\n",$warn);
			echo">_{$putih} $warn\n";
			garis();
		}
	}
	preg_match_all('#<p class="message"><i class="fas fa-check green"></i>(.*?)</p>#is',$res,$has);
	if($has[1]){
		for($i=0;$i<count($has[1]);$i++){
			fast("{$hijau}>_ {$putih}".$has[1][$i]."\n");
		}
		bal();
		garis();
	}
$res=ver();
	$tm = trim(strip_tags(explode('</span>',explode('Next payout in',$res)[1])[0]));
	if($tm){
		$minut=explode('m',$tm);
		if(strpos($minut[0],'s')==''){
			$tmer=$minut[0]*60;
			if($minut[1]){
				$tmer=$tmer+trim(str_replace('s','',$minut[1]));
			}
			if($tmer){
				ttmr($tmer);
			}
		}else{
			$tmer=trim(str_replace('s','',$minut[0]));
			if($tmer){
				ttmr($tmer);
			}
		}
	}
	
}