function verfir($cap,$challenge,$csrf){
$url="https://harshcoin.com/firewall/verify";
$data="adcopy_response=".$cap."&adcopy_challenge=".$challenge."&captchaType=solvemedia&csrf_token_name=".$csrf;
return curl($url,$data,head())[1];
}
